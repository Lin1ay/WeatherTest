    "dependencies": {
        "@types/jest": "24.0.13",
        "@types/node": "12.0.3",
        "@types/react": "16.8.19",
        "@types/react-dom": "16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "react-scripts": "3.0.1",
        "redux": "^4.0.1",
        "typesafe-actions": "^4.4.0",
        "typescript": "3.5.1"
    },
    "include": ["src", "public/TodayWeather"]
